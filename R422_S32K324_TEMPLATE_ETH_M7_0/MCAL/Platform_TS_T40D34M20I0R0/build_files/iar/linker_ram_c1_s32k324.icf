define symbol int_sram_stack_c0_size          = 0x00001000;    /* 4KB  */

define symbol int_sram_no_cacheable_c0_start  = 0x20418000;
define symbol int_sram_no_cacheable_c0_size   = 0x00008000;    /* 32KB, needs to include int_results  */

define symbol int_sram_shareable_start        = 0x20420000;
define symbol int_sram_shareable_size         = 0x00008000;    /* 32KB */

define symbol ram_end_c0                      = 0x20428000;

define symbol int_sram_c1_start               = 0x20430000;
